{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "tasker-app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angular-starter",
            "index": "src/ui/index.html",
            "main": "src/ui/main.ts",
            "polyfills": "src/ui/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/ui/favicon.ico",
              "src/ui/assets"
            ],
            "styles": [
              "src/ui/styles.scss",

              "src/ui/vendor/styles/bootstrap.scss",
              "src/ui/vendor/styles/appwork.scss",
              "src/ui/vendor/styles/theme-corporate.scss",
              "src/ui/vendor/styles/colors.scss",
              "src/ui/vendor/styles/uikit.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "src/ui/environments/environment.ts",
                "with": "src/ui/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "50kb",
                  "maximumError": "100kb"
                }
              ]
            },
            "serve": {
              "extractCss": true,
              "namedChunks": true,
              "vendorChunk": true,
              "commonChunk": true,
              "verbose": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tasker-app:build:serve"
          },
          "configurations": {
            "production": {
              "browserTarget": "tasker-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tasker-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/ui/test.ts",
            "polyfills": "src/ui/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "styles": [
              "src/ui/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/ui/favicon.ico",
              "src/ui/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "tasker-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tasker-app:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "tasker-app",
  "cli": {
    "analytics": false
  }
}
